<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mikey.attendance.model.SysCourseEntity" table="sys_course" schema="attendance">
        <id name="courseId" column="course_id"/>
        <property name="courseName" column="course_name"/>
        <property name="courseCode" column="course_code"/>
        <property name="teacherId" column="teacher_id"/>
    </class>
</hibernate-mapping>