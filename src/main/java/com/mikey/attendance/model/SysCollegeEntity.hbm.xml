<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mikey.attendance.model.SysCollegeEntity" table="sys_college" schema="attendance">
        <id name="collegeId" column="college_id"/>
        <property name="collegeName" column="college_name"/>
        <property name="collegeCode" column="college_code"/>
    </class>
</hibernate-mapping>